{
    "name": "condoedge/cms",
    "description": "A package to implement page editor inside your kompo application",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Anonimatrix\\PageEditor\\": "src/"
        },
        "files": [
            "src/helpers/components.php",
            "src/helpers/utils.php",
            "src/helpers/errors.php"
        ]
    },
    "authors": [
        {
            "name": "Ezequiel Imaz",
            "email": "tavelliezequiel@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": ">=5.6.0",
        "kompo/kompo": "^3.0",
        "condoedge/utils": "^0.2.12"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Anonimatrix\\PageEditor\\Providers\\PageEditorServiceProvider",
                "Anonimatrix\\PageEditor\\Providers\\PageItemServiceProvider",
                "Anonimatrix\\PageEditor\\Providers\\KnowledgeServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
